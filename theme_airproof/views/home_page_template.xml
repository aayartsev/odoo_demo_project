<odoo>
    <!-- 
        Если мы хотим, чтобы наша страница применялась только к нужном сайту в момент устновки темы
        нам нужно создвать именно шаблон, либо запись в модели theme.ir.ui.view. 
    
    -->
    <template id="homepage" name="Airproof Homepage" inherit_id="website.homepage">
        <xpath expr="//t[@t-call='website.layout']" position="replace">
            <t t-call="website.layout">
                <t t-set="additional_title" t-value="'Airproof Home'" />
                <t t-set="meta_description" t-value="'Meta Descrition'"/>
                <t t-set="pageName" t-value="'homepage-class'"/>
                
                <t t-set="no_header" t-value="false"/>
                <t t-set="no_footer" t-value="false"/>
                
                <div id="wrap" class="oe_structure oe_empty">
                    <div class="container">
                        <h1>Airproof Homepage</h1>
                    </div>
                </div>
            </t>
        </xpath>
    </template>
</odoo>